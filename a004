#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	int y;
 
 while (cin >> y )
 {
      
     if(y%400==0)
     {cout << "閏年" << endl;}
 
     else if(y%100==0)
     {cout << "平年" << endl;}
 
     else if(y%4==0)
     {cout << "閏年" << endl;}
     
     else {cout << "平年" << endl;
	 }
 
 }

	return 0;
}
