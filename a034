#include <iostream>
#include <cmath>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	int n;
	while(cin>>n){
		int t=0,num[20];
		while(n>0){
			num[t]=n%2;
			n/=2;
			t++;
		}
		for(int i=t-1;i>=0;i--){
			cout<<num[i];
		}
		cout<<endl;
	}
	return 0;
}
