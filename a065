#include <iostream>
#include <iomanip>
#include <math.h>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	char in[7];
	while(cin>>in){
		for(int i=1;i<=6;i++){
			cout<<abs(in[i]-in[i-1]);
		}
		cout<<endl;
	}
	return 0;
}
