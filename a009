#include <iostream>
#include <string.h>
#include <cmath>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	char n[100];
	while(cin>>n){
		for(int i=0;i<strlen(n);i++)
			cout<<(char)(n[i]-7);
		cout<<endl;
	}
	return 0;
}
