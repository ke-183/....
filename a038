#include<bits/stdc++.h>
using namespace std;
int main(int argc, char** argv) {

    ios_base::sync_with_stdio(0);
    //cin.tie(0);
    string s;
    int l;
    while(cin>>s){
        l=s.length();
        bool go=0;
        for(int i=l-1;i>=0;i--){
            if(s[i]!='0')
                go=1;
            if(go)
                cout<<s[i];
        }
        if(!go)
            cout<<0;
        cout<<'\n';
    }
    return 0;
}
