#include <iostream>
#include <iomanip>
#include <math.h>
#include <cmath>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	int n;
	while(cin>>n){
		int S1=0,S2=0,S3=0,a;
		for(int i=1;i<=n;i++){
			cin>>a;
			if(a%3==0){
				S1++;
			}
			else if(a%3==1){
				S2++;
			}
			else{
				S3++;
			}
		}
		cout<<S1<<" "<<S2<<" "<<S3<<endl;
	}
	return 0;
}
