#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	int M, D, S;
 
 while (cin >> M >> D )
 {
     S=(M*2+D)%3 ; 
 
     if(S==0)
     {cout << "普通" << endl;}
 
     else if(S==1)
     {cout << "吉" << endl;}
 
     else if(S==2)
     {cout << "大吉" << endl;}
 
 }

	return 0;
}
