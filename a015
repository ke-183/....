#include <iostream>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	int n,m;
	while(cin>>n>>m){
		int in[n][m];
		for(int i=0;i<n;i++)
			for(int j=0;j<m;j++)
				cin>>in[i][j];
		for(int i=0;i<m;i++){
			for(int j=0;j<n;j++)
				cout<<in[j][i]<<" ";
			cout<<endl;
		}
	}
	return 0;
}
