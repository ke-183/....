#include <iostream>
#include <cstring>
#include <cctype>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

using namespace std;

int main(int argc, char** argv) {
	
	string n;
	while(getline(cin,n)){
		string s;
		for(int i=n.length()-1;i>=0;i--)
			s+=n[i];
		if(n==s)
			cout<<"yes"<<endl;
		else
			cout<<"no"<<endl;
	}
	return 0;
}
